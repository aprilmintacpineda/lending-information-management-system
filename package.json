{
  "name": "1-lendingInfoSys",
  "version": "1.0.0",
  "description": "Lending Information System",
  "main": "main_process/bootstrap.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "node node_modules/webpack/bin/webpack.js",
    "bundle:watch": "node node_modules/webpack/bin/webpack.js --watch",
    "bundle:production": "SET PROD=1 && node node_modules/webpack/bin/webpack.js",
    "postinstall": "install-app-deps",
    "migration:init": "sequelize init",
    "migration:create": "sequelize migration:create --name",
    "migration:run": "sequelize db:migrate",
    "migration:rollback": "sequelize db:migrate:undo",
    "start": "electron -r babel-core/register .",
    "release": "build"
  },
  "build": {
    "appId": "com.lendingInfoSys",
    "files": [
      "resources/**/*",
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "keywords": [
    "lending information system"
  ],
  "author": "April Mintac Pineda <aprilmintacpineda@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "password-hash": "^1.2.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "sequelize": "^4.2.0",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "electron": "^1.4.10",
    "electron-builder": "^10.8.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "sass-loader": "^6.0.3",
    "sequelize-cli": "^2.7.0",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.0"
  }
}
